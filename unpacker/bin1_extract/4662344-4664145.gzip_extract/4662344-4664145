var ioStatus=new Object();(function(){ioStatus.fillwiredSum=function(c){var b=new apps.IoAdapterDO(c,"EthernetAdapter");$("#nS-wD-MacAddr").text(b.getMacAddr());var a;if(b.isConnected()){a="L@S#0364"}else{a="L@S#0188";$(".net-info-img > img").css("opacity",0.4)}$("#nS-wD-status").text(te.loc(a));var e=new apps.IoConfigDO(c);$("#nS-wD-HostName").text(e.hostname());var d=new apps.IoProtocolDO({pageFrag:c,name:"Eth0",complete:function(f){$("#nS-wD-IpAddr").text(f.getIpAddr("IPv4")||f.getLinkLocalIPv6Addr())}})};ioStatus.fillwirelessSum=function(d,h){h=h||"#net-wifi-sum-div";var f=new apps.IoAdapterDO(d,"WifiAdapter");var b;if(f.isConnected()){b="L@S#0364"}else{b="L@S#0188"}$("#nS-wS-status").text(te.loc(b));$("#nS-wS-MacAddr").text(f.getMacAddr());$("#nS-wS-Ssid").text(f.getSSID());var c=f.getSignalStrength();$(h+" .net-info-img").html(gui.emitHtml("div",{"class":"wifi-signal-icon-"+c},""));if(gui.isTheatreLNF()){var a=["No Signal","Low","Poor","Fair","Good","Excellent"];te.appendOffScreenText($(h+" .net-info-img"),"The Signal Strength for the  Wireless Network  is "+a[c])}var g=new apps.IoConfigDO(d);$("#nS-wS-HostName").text(g.hostname());var e=new apps.IoProtocolDO({pageFrag:d,name:"Wifi0",complete:function(i){$("#nS-wS-IpAddr").text(i.getIpAddr("IPv4")||i.getLinkLocalIPv6Addr())}})};ioStatus.fillDirectPrintSum=function(e){var d=new apps.NetAppsDO(e);var b=new apps.IoAdapterDO(e,"AccessPointAdapter");var f=new apps.NetAppsSecureDO(e);var c;if(d.getDirectPrintStatus()&&!b.isAdminDisabled()){c="L@S#0559"}else{c="L@S#0560"}$("#nS-Dp-status").text(te.loc(c));if(f&&f.hasProp("WirelessDirectConfig.AutoAcceptConnection")){$("#direct-title").html(gui.emitHtml("h2",{},te.loc("L@S#4777")))}else{$("#direct-title").html(gui.emitHtml("h2",{},te.loc("L@S#3159")))}var a=new apps.IoProfileDO({pageFrag:e,name:"Wifi1",complete:function(h){var g=h.getAdapterProfile();$("#nS-Dp-hostname").text(g.SSID);$("#nS-Dp-channel").text(g.Channel);if((f&&f.hasProp("WirelessDirectConfig.HideSSID"))&&(f.getJsObj().WirelessDirectConfig.HideSSID)){$("#nS-Dp-hostname").html(gui.emitHtml("span",{},"*"+te.loc("L@S#4804")+"*"))}}})}})();
