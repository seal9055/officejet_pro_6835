(function(){var c={dg_icon_wired_summary:"/images/dg_icon_wired_summary.gif"};apps.networkSum={};var a;apps.networkSum.preProcessTemplate=function(){a=this};apps.networkSum.onLoad=function(){var d;d=new apps.IoAdapterDO(a,"EthernetAdapter");if(d.isPresent()){ioStatus.fillwiredSum(a);b("#net-wired-sum-div","statusPage");if(gui.isTheatreLNF()&&gui.isMobileView()){$(".net-info-img img").attr("src","/images/theme-gray/mobile/dg_icon_wired_summary.png")}$("#net-wired-sum-div").show()}d=new apps.IoAdapterDO(a,"WifiAdapter");if(d.isPresent()){ioStatus.fillwirelessSum(a);b("#net-wifi-sum-div","defaultProfilePage");$("#net-wifi-sum-div").show()}var e=new apps.NetAppsDO(a);d=new apps.IoAdapterDO(a,"AccessPointAdapter");if(e.hasService("DirectPrint")&&d.isPresent()){ioStatus.fillDirectPrintSum(a);$("#net-dirPrint-sum-div").show()}};function b(f,e){if(!gui.isConsumerTheme()){return}var h=$(f);$(".net-info-ip-addr-label",h).html(te.loc("L@S#2926"));$(".net-info-ssid-label",h).html(te.loc("L@S#0130"));var d=$(".net-info-detailed-link",h);d.show();var i=$.hlp.getTmpDivId();var g=te.loc("L@S#2314");d.html(gui.emitHtml("a",{href:"#","class":"right",title:g},g)+gui.emitHtml("span",{id:i},""));$("a",d).click(function(){$.browseHistory.clickALink(e);return false});te.createSimplePrintJob(a,{jobId:"networkDiagnosticPage",btnText:"L@S#1101",divId:i})}})();
